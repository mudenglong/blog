{# src/Redwood/WebBundle/Resources/views/Webhq/list.html.twig #}
{% extends 'RedwoodWebBundle::webLayout.html.twig' %}

{% block title %}绘图配置{% endblock %}

{% set nav = 'webhq' %}
{% set script_controller = 'webhq/list' %}

{% block wrapBox %}
    <table id="config-table" class="table table-striped table-hover" data-search-form="#jswidget-search-form">
        <thead>
            <tr>
                <th>配置id</th>
                <th>名称</th>
                <th>简要描述</th>
                <th>配置详情</th>
                <th>操作</th>
            </tr>
        </thead>
        <tbody>
            {% for data in datas %}
                {% include 'RedwoodWebBundle:Webhq:list-tr.html.twig' with {data:data} %}
            {% endfor %}
        </tbody>
    </table>
{% endblock %}
