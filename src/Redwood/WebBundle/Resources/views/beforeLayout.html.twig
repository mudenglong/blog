{# src/Redwood/WebBundle/Resources/views/beforeLayout.html.twig #}
<!DOCTYPE HTML>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <title>{% block title %}Register{% endblock %}</title>
   {#  <link rel="stylesheet" href="{{ asset('bundles/redwoodweb/css/kippt.css') }}" /> #}
    <link rel="stylesheet" href="{{ asset('libs/bootstrap/3.0.3/css/bootstrap.css') }}" />
    {# <link href="{{ asset('assets/css/common.css') }}" rel="stylesheet" />
    <link href="{{ asset('bundles/redwoodweb/css/web.css') }}" rel="stylesheet" /> #}
 
</head>

<body>
            
    <ul>
        <li><a href="{{ url("login") }}">Log in</a></li>
        <li class="signup"><a href="{{ url("register") }}">Sign up</a></li>
    </ul>

    {% block introduce %}  
    
    {% endblock %}

    {% block content %}  
    
    {% endblock %}
    <script>
        var app = {};
        app.debug = {% if app.debug %}true{% else %}false{% endif %};
        app.version = '{{ assets_version() }}';

        {% if script_controller is defined %}
            app.controller = '{{ script_controller }}';
        {% endif %}

    </script>
    <script src="{{ asset('libs/seajs/2.1.1/sea.js') }}"></script>
    <script src="{{ asset('libs/seajs-global-config.js') }}"></script>       
    <script type="text/javascript">
        seajs.use('{{ asset('bundles/redwoodweb/js/app.js') }}');
    </script>

</body>
</html>